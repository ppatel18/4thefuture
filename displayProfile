<body >
<h1> Hi </h1>
<div id="listdata">

</div>


<script>
    $(document).ready(function(){
        (function() {
            // no data for other categories besides science
            var ourAPI = "http://34.209.38.109:8080/location/ms/rest/get/uid1";
            $.getJSON( ourAPI, {
                format: "json"
            })
                .done(function( data ) {
                    console.log(data);
                    $("<p>" + "Name: " + data.name + "</p>").appendTo("#listdata");
                    $("<p>" + "Country: " + data.country + "</p>").appendTo("#listdata");
                    $("<p>" + "Address: " + data.address + "</p>").appendTo("#listdata");
                    $("<p>" + "Age: " + data.age + "</p>").appendTo("#listdata");
                    $("<p>" + "Ambition: " + data.ambition + "</p>").appendTo("#listdata");
                    $("<p>" + "Country: " + data.country + "</p>").appendTo("#listdata");
                    $("<p>" + "IsInstructor: " + data.isInstructor + "</p>").appendTo("#listdata");
                    $("<p>" + "IsStudent: " + data.isStudent + "</p>").appendTo("#listdata");
                    $("<p>" + "sex " + data.sex + "</p>").appendTo("#listdata");
                    $("<p>" + "Skills " + data.skills + "</p>").appendTo("#listdata");
                    $("<p>" + "uid " + data.uid + "</p>").appendTo("#listdata");

                });
        })();
    });
</script>
